def calculate(operator, num_1, num_2):
    if operator == "multiply":
        return num_1 * num_2
    elif operator == "divide":
        return int(num_1 / num_2)
    elif operator == "add":
        return  num_1 + num_2
    elif operator == "subtract":
        return  num_1 - num_2
input_operator = input()
num_one = int(input())
num_two = int(input())

print(calculate(input_operator,num_one,num_two))
